{
    "name": "bilty/bilty-services",
    "description": "A Laravel Nova tool used to save shorten url links in your bitly accout through nova-admin panel.",
    "authors": [{
            "name": "Khaled Gamal",
            "email": "khaledgamalsabbah@gmail.com"
        }],
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Bilty\\BiltyServices\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bilty\\BiltyServices\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
